#
# Chemical Symbol and orbital
#

chemical.symbol             H
orbital                     2p
spin.orbital                beta

#
# Calculation type
#

eq.type                     default         # sch|sdirac|dirac default = sch

#
# Parameters for solving 1D-differential equations
#

grid.xmin                  -7.5             # default = -7.0 rmin(a.u.) = exp(grid.xmin)
grid.xmax                   6.0             # default = 5.0 rmax(a.u.) = exp(grid.xmax)
grid.num                    100000          # default = 20000
eps                         5.0E-16         # default = 1.0E-15
solver.type         ADAMS_BASHFORTH_MOULTON # Adams_Bashforth_Moulton|Bulirsch_Stoer|Controlled_Runge_Kutta default = Bulirsch_Stoer
search.LowerE               Auto            # default = Auto
num.of.partition            300             # default = 300
matching.point.ratio        0.67            # default = 0.67
